#include <iostream>

using namespace std;


//Задача 1
//Напиши програма каде ќе се пресмета сума од сите вредности помеѓу 1-30 кои се
//деливи со 10 и производ од сите останати вредности. Резултатот да се отпечати.


int main() {

    int broj;
    int suma = 0;
    int proizvod = 1;
   

    for(broj = 1; broj<=30; broj++){
        if(broj%10 ==0){
         suma+=broj;
        }
        else{
            proizvod *= broj;
        }
    }
    cout<<"Suma na broevi delivi so 10: "<<suma<<endl;
    cout<<"Proizvod od ostanatite broevi"<<proizvod<<endl;

    return 0;
}

